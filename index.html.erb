        <hr>
        <h2 id="Teaching">Teaching</h2>
        <table class='pastCourses'>
          <tr>
            <th>Academic Year</th>
            <th>Fall</th>
            <th>Winter</th>
            <th>Spring/Summer</th>
          </tr>
          <% courses.keys.sort {|a, b| b <=>a }.each do |year| %>
            <tr>
              <td class='ay'><%= "#{year}-#{year+1}" %></td>
              <td> <%= $d.general.semesterCourseList($d.semester.shortName('F', year), courses[year][:fall]) %></td>
              <td> <%= $d.general.semesterCourseList($d.semester.shortName('W', year+1), courses[year][:winter]) %></td>
              <td> <%= $d.general.semesterCourseList($d.semester.shortName('S', year+1), courses[year][:ss]) %></td>
            </tr>
          <% end %>
        </table>
        <hr>
