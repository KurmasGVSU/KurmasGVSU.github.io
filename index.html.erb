        }

      .pastCourses th {
        padding-right: 35px;
        text-align: left;
      }

      .pastCourses td {
        text-align: left;
        vertical-align: top;
        padding-right: 35px;
        padding-top: 3px;
        padding-bottom: 3px;
      }

        .pastCourses tr:nth-child(even) {
      background-color: #f2f2f2;
        }
    </style>
